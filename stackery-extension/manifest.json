{
  "manifest_version": 3,
  "name": "Stackery",
  "description": "Scrap werbsites url",
  "version": "1.0",
  "permissions": ["clipboardWrite", "tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "src/icons/stack_16.png",
    "32": "src/icons/stack_32.png",
    "48": "src/icons/stack_48.png",
    "128": "src/icons/stack_128.png"
  },
  "permissions": ["tabs"]
}
